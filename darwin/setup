DOTFILES=$(readlink -f $(dirname $0)/..)
source $DOTFILES/scripts/symlinks.sh
source $DOTFILES/scripts/log.sh

# Setup default configurations
$DOTFILES/unix/setup

# Setup specific configurations
einfo "Creating darwin symlinks"
replace $DOTFILES darwin

# Setup macos preferences and defaults
einfo "Setting up macos preferences and defaults"
$DOTFILES/darwin/.macos
